class Main {
  public static void main(String[] args) {

    try {

      // code that generate exception
      int divideByZero = 100 / 0;
      System.out.println("Whatever you want ");
    }
    
    catch (ArithmeticException e) {
      System.out.println("ArithmeticException => " + e.getMessage());
    }
  }
}
